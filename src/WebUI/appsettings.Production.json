{
  "UseInMemoryDatabase": false,
  "ConnectionStrings": {
    "AZURE_MYSQL_CONNECTIONSTRING": "Server=blabla; User ID=user; Password=password; Database=echopost"
  },
  "Logging": {
    "LogLevel": {
      "Default": "Debug",
      "System": "Information",
      "Microsoft": "Information"
    }
  },
  "IdentityServer": {
    "Clients": {
      "EchoPost.WebUI": {
        "Profile": "SPA",
        "RedirectUri": "https://echopost-angular-client.azurewebsites.net/authentication/login-callback",
        "LogoutUri": "http://echopost-angular-client.azurewebsites.net/authentication/logout-callback",
        "IdentityTokenLifetime" : "900",
        "AccessTokenLifetime" : "900"
      }
    },
    "Key": {
      "Type": "Store",
      "StoreName": "My",
      "StoreLocation": "CurrentUser",
      "Name": "CN=MyApplication"
    },
    "IssuerUri":"https://echopost-server.azurewebsites.net"
  }
}
